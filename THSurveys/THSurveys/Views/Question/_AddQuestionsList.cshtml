@model IEnumerable<THSurveys.Models.Question.AddQuestionsListViewModel>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SequenceNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SequenceNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @*@Html.ActionLink("Move Up", "Move", new {questions = Model })*@
            @*@Html.ActionLink("Move Down", "Down", new {questions = Model })*@
        </td>
    </tr>
}

</table>
